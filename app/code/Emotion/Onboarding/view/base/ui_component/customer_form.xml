<?xml version="1.0" encoding="UTF-8" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="customer">
        <field name="extension_attributes.new_onboarding_name" formElement="input">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">extension_attributes.new_onboarding_name</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Onboarding name</label>
            </settings>
        </field>
        <field name="extension_attributes.onboarding_lastname" formElement="input">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">extension_attributes.onboarding_lastname</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Onboarding lastname</label>
            </settings>
        </field>
    </fieldset>
</form>
